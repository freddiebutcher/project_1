<%= form_for @post do |f| %>
<%= f.label :episode_name, :required => true %>
<%= f.text_field :episode_name, :required => true %>
<%= f.label :show_name %>
<%= f.text_field :show_name %>
<%= f.label :episode_description %>
<%= f.text_field :episode_description %>
<%= f.label :pod_link %>
<%= f.text_field :pod_link %>
<%= f.label :img_link %>
<%= f.cl_image_upload :img_link %>
<%= f.submit %>
<% end %>
