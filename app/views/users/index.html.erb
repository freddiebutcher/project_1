<h1>All Users</h1>
<p>List of users</p>

<% @users.each do |user| %>

<ul>
  <li>Username: <%= link_to user.username, user %></li>
  <li>First name: <%= user.first_name %></li>
  <li>Last name: <%= user.last_name %></li>
  <li>Email: <%= user.email_address %></li>
  <li>User bio: <%= user.user_bio %></li>
</ul>
<% end %>
