

<!-- User Tiles -->
<div class="container2">
  <div class="pagetitle">
    <h2>Podstas</h2>
  </div>
  <div class="row">
    <% @users.each do |user| %>
    <div class="col-sm-4 cell">
      <ul>
        <div class = "indexcontainer">
          <div class="indexavatar">
            <%= cl_image_tag user.avatar, :class => "userindexavatarimage" unless user.avatar.nil? %>
          </div>
          <li><%= link_to user.username, user, :class => "userhandles"%></li>

        </div>
      </ul>
    </div>
    <% end %>
  </div>
</div>
