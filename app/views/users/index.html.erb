
<h1>USERS TEST</h1>

<!-- User Tiles -->
<div class="container">
  <div class="pagetitle">
    <h2>Users</h2>
  </div>
  <div class="row">
    <% @users.each do |user| %>
    <div class="col-sm-4 cell" style= "background-color: #fafafa">
      <ul>
        <div class = "indexcontainer">
          <div class="indexavatar">
            <%= cl_image_tag user.avatar, :class => "userindexavatarimage" unless user.avatar.nil? %>
          </div>
          <li><%= link_to user.username, user %></li>

        </div>
      </ul>
    </div>
    <% end %>
  </div>
</div>
