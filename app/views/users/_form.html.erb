<%= form_for @user do |f| %>
<%= f.label :email_address, :required => true %>
<%= f.text_field :email_address, :required => true %>
<%= f.label :password, :required => true %>
<%= f.text_field :password, :required => true %>
<%= f.label :first_name, :required => true %>
<%= f.text_field :first_name, :required => true %>
<%= f.label :last_name, :required => true %>
<%= f.text_field :last_name, :required => true %>
<%= f.label :user_bio, :required => true %>
<%= f.text_field :user_bio, :required => true %>
<%= end %>
