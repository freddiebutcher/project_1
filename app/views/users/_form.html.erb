<%= form_for @user, :html => {:multipart => true} do |f| %>
<%= f.label :email_address, :required => true %>
<%= f.text_field :email_address, :required => true %>
<%= f.label :password %>
<%= f.text_field :password %>
<%= f.label :first_name %>
<%= f.text_field :first_name %>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<%= f.label :user_bio %>
<%= f.text_field :user_bio %>
<%= f.label :avatar %>
<%= f.text_field :avatar %>
<%= f.submit %>
<% end %>
